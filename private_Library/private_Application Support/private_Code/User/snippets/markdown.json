{
  "Date header": {
    "body": [
      "## $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE - $0",
      ""
    ],
    "prefix": "dheader",
    "description": "Insert an H2 date header"
  },
  "Checked checkbox": {
    "prefix": "ChBox",
    "description": "an unchecked checkbox",
    "body": [
      "- [x] "
    ]
  },
  "Unchecked checkbox": {
    "prefix": "UnChBox",
    "description": "an unchecked checkbox",
    "body": [
      "- [ ] "
    ]
  },
  "Source url": {
    "prefix": "source",
    "description": "add source URL for notes",
    "body": "((source)[$1])$0"
  },
  "link": {
    "prefix": "link",
    "description": "formatted markdown link",
    "body": "[${2:display text}](${1:URL})$0"
  },
  "table": {
    "prefix": "table",
    "description": "new table template",
    "body": [
      "| $1 | $2 |",
      "| - | - |",
      "| $0 | |"
    ]
  },
  "see": {
    "prefix": "see",
    "description": "reference to link for more information",
    "body": "see: [${2:display text}](${1:URL})$0"
  },
  "image": {
    "prefix": "img",
    "description": "add an image tag",
    "body": "![${2:alt text}](/${1:path to image})$0"
  },
  "important": {
    "prefix": "important",
    "description": "Add GitHub Markdown IMPORTANT alert",
    "body": [
      "> [!IMPORTANT] ${1:title}",
      "> ",
      "> $0"
    ]
  },
  "caution": {
    "prefix": "caution",
    "description": "Add GitHub Markdown CAUTION alert",
    "body": [
      "> [!CAUTION] ${1:title}",
      "> ",
      "> $0"
    ]
  },
  "warning": {
    "prefix": "warning",
    "description": "Add GitHub Markdown WARNING alert",
    "body": [
      "> [!WARNING] ${1:title}",
      "> ",
      "> $0"
    ]
  },
  "note": {
    "prefix": "note",
    "description": "Add GitHub Markdown NOTE alert",
    "body": [
      "> [!NOTE] ${1:title}",
      "> ",
      "> $0"
    ]
  },
  "tip": {
    "prefix": "tip",
    "description": "Add GitHub Markdown TIP alert",
    "body": [
      "> [!TIP] ${1:title}",
      "> ",
      "> $0"
    ]
  },
