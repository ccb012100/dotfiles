{
	"add abbr": {
		"prefix": "abbr",
		"body": "abbr --add ${1:name} ${2:command}",
		"description": "add new abbr"
	},
	"function": {
		"prefix": "fn",
		"body": [
			"function ${1:name} --description '${2:description}'",
			"\t$0",
			"end"
		],
		"description": "add new function definition",
	},
	"scriptdir": {
		"prefix": "scriptdir",
		"body": "set script_dir (dirname (status --current-filename))",
		"description": "set variable with the directory of the running script"
	}
}